{
  "redirects": [
    {
      "source": "/",
      "destination": "https://jhqcat.com/",
      "permanent": true
    },
    {
      "source": "/:path*",
      "destination": "https://jhqcat.com/:path*",
      "permanent": true
    },
    {
      "source": "/posts/:path*",
      "destination": "https://jhqcat.com/blog/:path*",
      "permanent": true
    }
  ]
}
